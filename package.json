{
  "name": "Hospital-Sim",
  "version": "0.3.0",
  "private": true,
  "main": "app.js",
  "browserify-shim": {
    "react": "global:React",
    "react-dom": "global:ReactDOM",
    "aframe": "global:AFRAME"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/custom_test_utilities/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 90,
        "lines": 87,
        "statements": 85
      }
    }
  },
  "dependencies": {
    "aframe": "^0.5.0",
    "aframe-mouse-cursor-component": "^0.4.1",
    "aframe-react": "^4.2.3",
    "fs": "0.0.2",
    "lodash": "^4.17.4",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-joyride": "^1.10.1",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-jest": "^20.0.3",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "browserify-shim": "^3.8.14",
    "enzyme": "^2.9.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-clean": "^0.3.1",
    "gulp-clone": "^1.0.0",
    "gulp-concat": "^2.6.0",
    "gulp-copy": "0.0.2",
    "gulp-cssmin": "^0.1.7",
    "gulp-print": "^2.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-replace-task": "^0.11.0",
    "gulp-sass": "^2.2.0",
    "gulp-uglify": "^2.1.2",
    "gulp-zip": "^3.2.0",
    "husky": "^0.13.4",
    "jest": "^20.0.4",
    "jsdom": "^11.0.0",
    "lint-staged": "^3.6.1",
    "prettier": "^1.4.4",
    "react-test-renderer": "^15.6.1",
    "run-sequence": "^1.2.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "yargs": "^4.5.0"
  },
  "scripts": {
    "precommit": "lint-staged",
    "test": "jest --coverage --verbose",
    "build": "gulp"
  },
  "lint-staged": {
    "*.js": [
      "prettier --use-tabs --jsx-bracket-same-line --write",
      "git add"
    ]
  },
  "author": "New Media"
}