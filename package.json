				"statements": 87
			}
		}
	},
	"materia": {
		"cleanName": "hospital-sim"
	},
	"dependencies": {
		"aframe": "^0.5.0",
		"aframe-click-drag-component":
			"git+ssh://git@clu.cdl.ucf.edu:materia/aframe-click-drag-component" ,
		"aframe-mouse-cursor-component": "^0.5.2",
		"aframe-react": "^4.2.3",
		"fs": "^0.0.2",
		"lodash": "^4.17.4",
		"materia-widget-development-kit":
			"git+ssh://git@clu.cdl.ucf.edu:materia/materia-widget-development-kit#semver:>=1.0.7 <1.10.0",
		"react": "^15.5.4",
		"react-dom": "^15.5.4",
		"react-joyride": "^1.10.1",
		"react-redux": "^5.0.5",
		"redux": "^3.6.0"
	},
	"devDependencies": {
		"babel-core": "^6.25.0",
		"babel-jest": "^20.0.3",
		"babel-loader": "^7.1.1",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-3": "^6.24.1",
		"enzyme": "^2.9.1",
		"husky": "^0.13.4",
		"jest": "^20.0.4",
		"jsdom": "^11.0.0",
		"lint-staged": "^3.6.1",
		"prettier": "^1.4.4",
		"react-test-renderer": "^15.6.1"
	},
	"scripts": {
		"start": "webpack-dev-server",
		"build": "webpack",
		"build-prod": "webpack -p",
		"test": "jest --coverage --verbose"
	},
	"lint-staged": {
		"*.js": ["prettier --use-tabs --jsx-bracket-same-line --write", "git add"]
	},
	"author": "New Media"
