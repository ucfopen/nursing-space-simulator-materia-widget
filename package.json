{
	"name": "Hospital-Sim",
	"version": "0.2.0",
	"private": true,
	"main": "app.js",
	"jest": {
		"collectCoverageFrom": ["src/**/*.js"],
		"testPathIgnorePatterns": ["/node_modules/", "/custom_test_utilities/"],
		"coverageThreshold": {
			"global": {
				"branches": 97,
				"functions": 91,
